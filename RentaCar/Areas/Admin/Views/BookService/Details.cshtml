@using RentaCar.Areas.Admin.ViewModels.BookService
@model BookServiceDetailsViewModel
<div class="col-lg-12 grid-margin ">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Book Service Details</h4>
            <h3>Photos:</h3>
            <div>
            </div>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>mainPhoto</th>
                            <th>Id</th>
                            <th>Title</th>
                            <th>Description</th>
                         


                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><img src="~/assets/img/@Model.PhotoPath" /></td>
                            <td>@Model.Id</td>
                            <td>@Model.Title</td>
                            <td>@Model.Description</td>

                        </tr>
                    </tbody>
                </table>
                <a asp-action="index" asp-controller="bookservice" class="btn btn-info">Cancel</a>
            </div>
        </div>
    </div>
</div>