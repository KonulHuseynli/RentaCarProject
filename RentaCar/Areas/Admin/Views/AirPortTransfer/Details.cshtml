@using RentaCar.Areas.Admin.ViewModels.AirPortTransfer
@model AirPortTransferDetailsViewModel
<div class="col-lg-12 grid-margin ">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">AirPortTransfer Details</h4>
       
            <div>
            </div>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>MainPhoto</th>
                            <th>Id</th>
                            <th>Brand</th>
                            <th>Model</th>
                            <th>Description</th>
                            <th>Price</th>



                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><img src="~/assets/img/@Model.MainPhoto" /></td>
                            <td>@Model.Id</td>
                            <td>@Model.CreateDate.ToString("dd/MM/yyyy")</td>
                            <td>@Model.Brand</td>
                            <td>@Model.Model</td>
                            <td>@Model.Description</td>
                            <td>@Model.Price</td>


                        </tr>
                    </tbody>
                </table>
                <a asp-action="index" asp-controller="airporttransfer" class="btn btn-info">Cancel</a>
            </div>
        </div>
    </div>
</div>